<https://slovník.gov.cz/základní/pojem/je-vlastností> a owl:ObjectProperty;
  owl:inverseOf <https://slovník.gov.cz/základní/pojem/má-vlastnost> .

<https://slovník.gov.cz/základní/pojem/má-vlastnost> a owl:ObjectProperty .

<https://slovník.gov.cz/základní/pojem/je-ve-vztahu> a owl:ObjectProperty .

<https://slovník.gov.cz/základní/pojem/má-vztažený-prvek> a owl:ObjectProperty;
  owl:inverseOf <https://slovník.gov.cz/základní/pojem/je-ve-vztahu> .

d-sgov-psp:model a a-popis-dat-pojem:model, owl:Ontology;
  dcterms:created "2020-07-28";
  dcterms:title "Datový slovník Poslanecké sněmovny Parlamentu České republiky - model"@cs;
  bibo:status "Specification"@en, "Specifikace"@cs;
  vann:preferredNamespacePrefix "d-sgov-psp-pojem";
  vann:preferredNamespaceUri "https://slovník.gov.cz/datový/psp/pojem/";
  owl:imports d-sgov-psp:glosář;
  owl:versionIRI <https://slovník.gov.cz/datový/psp/model/verze/0.0.1> .

d-sgov-psp-pojem:má-zkrácený-název-tisku a owl:Class, <https://slovník.gov.cz/základní/pojem/typ-vlastnosti>;
  rdfs:subClassOf <https://slovník.gov.cz/základní/pojem/vlastnost>, [ a owl:Restriction;
      owl:allValuesFrom <https://slovník.gov.cz/legislativní/sbírka/90/1995/pojem/sněmovní-tisk>;
      owl:onProperty <https://slovník.gov.cz/základní/pojem/je-vlastností>
    ], [ a owl:Restriction;
      owl:onProperty <https://slovník.gov.cz/základní/pojem/je-vlastností>;
      owl:someValuesFrom <https://slovník.gov.cz/legislativní/sbírka/90/1995/pojem/sněmovní-tisk>
    ] .

d-sgov-psp-pojem:má-url-stránky-tisku a owl:Class, <https://slovník.gov.cz/základní/pojem/typ-vlastnosti>;
  rdfs:subClassOf <https://slovník.gov.cz/základní/pojem/vlastnost>, [ a owl:Restriction;
      owl:onProperty <https://slovník.gov.cz/základní/pojem/je-vlastností>;
      owl:someValuesFrom <https://slovník.gov.cz/legislativní/sbírka/90/1995/pojem/sněmovní-tisk>
    ], [ a owl:Restriction;
      owl:allValuesFrom <https://slovník.gov.cz/legislativní/sbírka/90/1995/pojem/sněmovní-tisk>;
      owl:onProperty <https://slovník.gov.cz/základní/pojem/je-vlastností>
    ] .

skos:prefLabel rdfs:subPropertyOf rdfs:label .
